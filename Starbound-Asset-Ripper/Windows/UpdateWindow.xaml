<Window x:Name="UpdateWindow1" x:Class="Starbound_Asset_Ripper.Windows.UpdateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Starbound_Asset_Ripper.Windows"
        mc:Ignorable="d"
        Title="Checking for an update..." Height="155.002" Width="318.395" Background="#FF252525" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="CancelBtn" Content="Cancel" HorizontalAlignment="Left" Margin="10,71,0,0" VerticalAlignment="Top" Width="281"/>
        <ProgressBar x:Name="TimeoutProgressBar" HorizontalAlignment="Left" Height="22" Margin="71,40,0,0" VerticalAlignment="Top" Width="220" Maximum="5" Value="-1"/>
        <Label Content="Timeout:" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.531,0.328"/>
        <Label Content="Checking for an update - Times out after 5 seconds" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
